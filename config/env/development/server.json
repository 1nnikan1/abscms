{
  "host": "${process.env.SERVER_HOST || '0.0.0.0'}",
  "port": "${process.env.SERVER_PORT || 1337}",
  "production": false,
  "proxy": {
    "enabled": false
  },
  "cron": {
    "enabled": false
  },
  "admin": {
    "autoOpen": true,
    "auth": {
      "secret": "${process.env.ADMIN_JWT_SECRET} || ''"
    },
    "watchIgnoreFiles": ["**/private/**"]
  }
}